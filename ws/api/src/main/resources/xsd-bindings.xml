<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings jaxb:version="2.1"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">
	<jaxb:bindings schemaLocation="hr.xsd" node="/xsd:schema">
		<jaxb:globalBindings>
			<!-- customize mapping types -->
			<jaxb:javaType name="java.util.Date" xmlType="xsd:date"
				parseMethod="org.randompage.samples.ws.adapters.DateAdapter.parseDate"
				printMethod="org.randompage.samples.ws.adapters.DateAdapter.printDate" />
			<!-- enable XML root elements -->
			<xjc:simple />
		</jaxb:globalBindings>
		<jaxb:schemaBindings>
			<!-- java package -->
			<jaxb:package name="org.randompage.samples.ws.api" />
		</jaxb:schemaBindings>
	</jaxb:bindings>
</jaxb:bindings>